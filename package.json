{
  "name": "vnx-architype",
  "version": "1.0.0",
  "private": true,
  "description": "VNX-Architype architectural super-app monorepo",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:migrate": "cd packages/architype-db && supabase db push",
    "db:reset": "cd packages/architype-db && supabase db reset"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": { "node": ">=18.0.0", "pnpm": ">=8.0.0" },
  "workspaces": ["apps/*", "packages/*"]
}
